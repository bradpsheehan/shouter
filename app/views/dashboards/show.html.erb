<%= render 'profile_bar' %>
<div class="col-md-8 content-well">
  <%= shout_form_for(TextShout) do |form| %>
    <%= form.text_field :body, placeholder: "Shout here!" %>
  <% end %>

  <%= shout_form_for(PhotoShout) do |form| %>
    <%= form.file_field :photo %>
  <% end %>

  <div class="shout-feed">
    <%= render @dashboard.feed %>
  </div>
</div>
